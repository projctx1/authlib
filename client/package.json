{
  "name": "@yourcompany/vue-auth-sdk",
  "version": "1.0.0",
  "description": "Comprehensive Vue 3 authentication SDK with customizable components",
  "main": "dist/vue-auth-sdk.cjs.js",
  "module": "dist/vue-auth-sdk.es.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "exports": {
    ".": {
      "import": "./dist/vue-auth-sdk.es.js",
      "require": "./dist/vue-auth-sdk.cjs.js"
    },
    "./components": "./dist/components-only.es.js",
    "./composables": "./dist/composables-only.es.js",
    "./styles": "./dist/style.css"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:lib": "rollup -c",
    "preview": "vite preview",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "test": "vitest",
    "serve": "npm run dev"
  },
  "keywords": [
    "vue",
    "vue3",
    "authentication",
    "auth",
    "login",
    "sdk",
    "components",
    "security"
  ],
  "author": "Your Company",
  "license": "MIT",
  "peerDependencies": {
    "vue": "^3.3.0",
    "vuex": "^4.0.0",
    "vue-router": "^4.0.0"
  },
  "dependencies": {
    "axios": "^1.12.2"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.0",
    "@rollup/plugin-node-resolve": "^15.0.0",
    "@rollup/plugin-replace": "^5.0.0",
    "@rollup/plugin-terser": "^0.4.0",
    "@vitejs/plugin-vue": "^4.5.0",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/test-utils": "^2.4.0",
    "eslint": "^8.55.0",
    "eslint-plugin-vue": "^9.18.1",
    "jsdom": "^22.0.0",
    "prettier": "^3.1.0",
    "rollup": "^4.0.0",
    "rollup-plugin-vue": "^6.0.0",
    "sass": "^1.77.0",
    "vite": "^5.0.0",
    "vitest": "^0.34.0",
    "vue": "^3.3.8",
    "vuex": "^4.1.0",
    "vue-router": "^4.2.5"
  }
}
